<service-test>
  <unique-id>nostalgic-booth</unique-id>
  <test-client class="embedded-jmx-test-client">
    <shared-components>
      <connections/>
      <services/>
    </shared-components>
    <shared-components-provider>
      <services/>
    </shared-components-provider>
  </test-client>
  <helpers/>
  <test-list>
    <unique-id>Azure Cosmos DB Tests</unique-id>
    <test>
      <unique-id>cosmos db service@Cosmos DB wolkflow</unique-id>
      <test-case>
        <unique-id>cosmos db test case</unique-id>
        <input-message-provider>
          <metadata-provider class="empty-metadata-provider"/>
          <payload-provider class="empty-payload-provider"/>
        </input-message-provider>
        <assertions>
          <assert-metadata-key-exists>
            <key>x-ms-date</key>
          </assert-metadata-key-exists>
          <assert-metadata-key-exists>
            <key>Authorization</key>
          </assert-metadata-key-exists>
        </assertions>
      </test-case>
      <service-to-test>
        <source class="inline-source">
          <xml><![CDATA[<cosmosdb-authorization-header>
  <unique-id>cosmos db service</unique-id>
  <http-verb>GET</http-verb>
  <master-key>NWJjMTgzYWItZjkyNS00YWQwLTlkNzMtZjc4NmU0YzQzYjAxCg==</master-key>
  <resource-type>dbs</resource-type>
  <resource-id>dbs/MyDatabase/colls/MyCollection</resource-id>
</cosmosdb-authorization-header>]]></xml>
        </source>
        <preprocessors/>
      </service-to-test>
    </test>
  </test-list>
</service-test>
